{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "api/generate.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
